{
    "name": "vkolegov/dashamail-php",
    "description": "DashaMail PHP library",
    "type": "library",
    "require": {
        "php": "^7.1 || ^8.0",
        "ext-curl": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "vlucas/phpdotenv": "^3.0",
        "gebn/brush": "^1.2",
        "fakerphp/faker": "^1.10"
    },
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Vladislav Kolegov",
            "email": "v.kolegov@outlook.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "VKolegov\\DashaMail\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "VKolegov\\DashaMail\\Tests\\": "tests/"
        }
    }
}
